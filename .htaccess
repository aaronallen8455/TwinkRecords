RewriteEngine On
RewriteBase /twink

#force non-www version
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]
#hooks
RewriteRule ^calendar/(\d+)/?$ index.php?page=calendar&event=$1
RewriteRule ^admin/?$ admin/index.php
RewriteRule ^admin/([\w]+)/?$ admin/$1.php
RewriteRule ^admin/([\w]+)/(\d)+/?$ admin/$1.php?id=$2
RewriteRule ^/?$ index.php?page=front
RewriteRule ^([\w\d_\-]+)/?$ index.php?page=$1